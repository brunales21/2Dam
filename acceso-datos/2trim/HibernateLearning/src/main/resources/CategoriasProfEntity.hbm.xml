<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entities.CategoriasProfEntity" table="categorias_prof" schema="empleados">
        <id name="idCatProf">
            <column name="id_cat_prof" sql-type="varchar(3)" length="3"/>
        </id>
        <property name="minSalario">
            <column name="min_salario" sql-type="int" not-null="true"/>
        </property>
        <property name="maxSalario">
            <column name="max_salario" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>