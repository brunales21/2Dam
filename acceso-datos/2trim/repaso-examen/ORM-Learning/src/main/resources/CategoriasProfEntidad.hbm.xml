<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entidades.CategoriasProfEntidad" table="categorias_prof" schema="empleados">
        <id name="idCatProf" column="id_cat_prof"/>
        <property name="minSalario" column="min_salario"/>
        <property name="maxSalario" column="max_salario"/>
    </class>
</hibernate-mapping>